{
    "settings": {
        "import/resolver": {
            "node": {
                "paths": ["./src"]
            }
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "jest": true
    },
    "extends": "eslint-config-unosquare",
    "plugins": ["eslint-plugin-prettier"],
    "rules": {
        "no-extend-native": 0,
        "no-param-reassign": 0,
        "import/first": 0,
        "class-methods-use-this": 0,
        "camelcase": 0,
        "react-hooks/exhaustive-deps": 0
    }
}
