{
  "name": "tubular-fabric",
  "version": "0.0.3",
  "description": "Unosquare Tubular for Fabric",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --p ./tsconfig.json",
    "start": "webpack && webpack-dev-server",
    "check": "npm run build && npm test",
    "lint": "tsc --p ./tsconfig.json --noEmit && eslint ./src/**/*.ts --quiet",
    "lint:fix": "tsc --p ./tsconfig.json --noEmit && eslint ./src/**/*.ts --quiet --fix",
    "test": "jest"
  },
  "jest": {
    "automock": false,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/test/**/DataGrid.spec.(ts|tsx)"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false,
        "tsConfig": "tsconfig.base.json"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unosquare/tubular-fabric.git"
  },
  "author": "Unosquare",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unosquare/tubular-fabric/issues"
  },
  "homepage": "https://github.com/unosquare/tubular-fabric#readme",
  "dependencies": {
    "office-ui-fabric-react": "^7.105.2",
    "tubular-common": "^3.2.0",
    "tubular-react-common": "1.0.20"
  },
  "devDependencies": {
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "html-webpack-plugin": "^4.0.0",
    "prettier": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "files": [
    "dist"
  ]
}
